{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "appName": {
      "value": "canvas-kth-sandboxes-prod"
    },
    "secretNames": {
      "value": [
        "CANVAS_DEVELOPER_KEY_ID",
        "CANVAS_DEVELOPER_KEY_SECRET",
        "SESSION_SECRET"
      ]
    },
    "environmentVariables": {
      "value": {
        "CANVAS_API_URL":"https://canvas.kth.se/",
        "PROXY_HOST": "https://app.kth.se"
      }
    },
    "monitorPath": {
      "value": "/canvas-kth-sandboxes/_monitor"
    },
    "websitePort": {
      "value": "3000"
    },
    "deploySlot": {
      "value": true
    }
  }
}
